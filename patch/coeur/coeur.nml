&RUN_PARAMS 
hydro=.true.
poisson=.true.
ncontrol=1
nsubcycle=1,1,10*2
nrestart=0
verbose=.false.
nremap=1
cost_weighting=.false.
/

&AMR_PARAMS 
levelmin=7
levelmax=16
ngridmax=1000000
nexpand=10*6,
boxlen=1.0
/

&INIT_PARAMS
/

&OUTPUT_PARAMS
